struct test {
    fn init : self {
      self.x = 0;
    }

    fn print_x : self {
      println(self.x);
    }

    fn increment : self {
      self.x = self.x + 1;
      return self;
    }
}

println("Creating test instance...");
let t = test();
println(f"Test instance created: {t}");
println(f"Initial value of x: {t.x}");
println("Calling print_x method:");
t.print_x();
println("Incrementing x...");
t.increment();
println(f"Value of x after increment: {t.x}");
println("Calling print_x method again:");
t.print_x();
println("All tests passed!");
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Walrus VS Code Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode/walrus"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode/walrus/**/*.js"
      ],
      "preLaunchTask": "Build walrus-lsp"
    }
  ]
}
